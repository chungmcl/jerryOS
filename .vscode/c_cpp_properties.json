{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/src/include/*",
                "${workspaceFolder}/src/jerryLibc/*",
                "${workspaceFolder}/src/libfdtLite/*"
            ],
            "defines": [
                "RUST"
            ],
            "compilerPath": "/opt/homebrew/opt/llvm/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64",
            "configurationProvider": "ms-vscode.makefile-tools",
            "compilerArgs": [
                "-I${workspaceFolder}/src/jerryLibc/include/",
                "-I${workspaceFolder}/src/libfdtLite/include/"
            ]
        }
    ],
    "version": 4
}